
<?xml version="1.0" encoding="UTF-8"?>
<extentreports>
    <configuration>
        <!-- report theme -->
        <!-- STANDARD, DARK -->
        <!--changing the theme from STANDARD to DARK-->
        <theme>DARK</theme>

        <!-- document encoding -->
        <!-- defaults to UTF-8 -->
        <encoding>UTF-8</encoding>

        <!-- protocol for script and stylesheets -->
        <!-- defaults to https -->
        <!-- HTTP, HTTPS -->
        <protocol>HTTPS</protocol>

        <!-- offline report -->
        <timelineEnabled>true</timelineEnabled>

        <!-- offline report -->
        <enableOfflineMode>false</enableOfflineMode>

        <!-- use thumbnails for base64 images -->
        <!-- this may slowdown viewing tests -->
        <thumbnailForBase64>true</thumbnailForBase64>

        <!-- title of the document -->
        <documentTitle>I am happy today</documentTitle>

        <!-- report name - displayed at top-nav -->
        <reportName>LearningReports</reportName>

        <!-- timestamp format -->
        <timeStampFormat>dd-MM-yyyy hh:mm:ss</timeStampFormat>

        <!-- custom javascript -->
        <scripts>
            <![CDATA[
        $(document).ready(function() {
        });
      ]]>
        </scripts>

        <!-- custom styles -->
        <styles>
            <![CDATA[

      ]]>
        </styles>

    </configuration>
</extentreports>
